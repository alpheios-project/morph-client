{
  "name": "alpheios-morph-client",
  "version": "1.1.1",
  "description": "Alpheios Morphology Client Library",
  "main": "dist/alpheios-morph-client.standalone.js",
  "moduleExternal": "dist/alpheios-morph-client.module-external.js",
  "scripts": {
    "test": "standard --fix && jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "build": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack all",
    "build-prod": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack production",
    "build-dev": "standard --fix && node ./node_modules/alpheios-node-build/dist/scripts/build.js webpack development",
    "lint": "standard --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpheios-project/morph-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alpheios-project/morph-client/issues"
  },
  "homepage": "https://github.com/alpheios-project/morph-client#readme",
  "devDependencies": {
    "alpheios-node-build": "github:alpheios-project/node-build",
    "alpheios-data-models": "github:alpheios-project/data-models",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.4.0",
    "copy-webpack-plugin": "^4.5.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.0",
    "eslint-plugin-import": "^2.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-optipng": "^5.2.1",
    "imagemin-svgo": "^6.0.0",
    "jest": "^22.4.3",
    "jest-fetch-mock": "1.3.3",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "parallel-webpack": "^2.3.0",
    "path": "^0.12.7",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.4",
    "raw-loader": "^0.5.1",
    "sass-loader": "^7.0.1",
    "semver": "^5.5.0",
    "source-map-loader": "^0.2.3",
    "standard": "^11.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.0.0-rc.2",
    "vue-style-loader": "^4.1.0",
    "vue-svg-loader": "^0.5.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {},
  "jest": {
    "verbose": true,
    "transformIgnorePatterns": [
      "node_modules/?!(alpheios-data-models)"
    ]
  },
  "standard": {
    "ignore": [
      "**/dist"
    ]
  }
}
